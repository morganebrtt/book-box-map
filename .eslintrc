{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": ["warn", {
        "endOfLine":"auto"
      }],
      "react/no-array-index-key": 0,
      "no-unused-vars": 0,
      "react/jsx-boolean-value": 0
    }
  }